<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Beta>False</Beta>
  <Category>Backup:</Category>
  <Name>Burp-ui</Name>
  <Description>
     pschiffe/burp-ui[br]
  </Description>
  <Overview>
    Burp backup ui [br]
  </Overview>
  <Support>http://lime-technology.com/forum/index.php</Support>
  <Registry>https://hub.docker.com/r/pschiffe/burp-ui</Registry>
  <GitHub>https://github.com/pschiffe/docker-burp</GitHub>
  <Repository>pschiffe/burp-ui</Repository>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>4971-4972</HostPort>
        <ContainerPort>4971-4972</ContainerPort>
        <Protocol>tcp</Protocol>
	  </Port>
	  <Port>
        <HostPort>5000</HostPort>
        <ContainerPort>5000</ContainerPort>
        <Protocol>tcp</Protocol>
	  </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>DEBUG</Name>
      <Value>false</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/burp-ui/config</HostDir>
      <ContainerDir>/etc/burp</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/burp-ui/data</HostDir>
      <ContainerDir>/var/spool/burp</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/etc/localtime</HostDir>
      <ContainerDir>/etc/localtime</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
	<Volume>
      <HostDir>/sys/fs/cgroup</HostDir>
      <ContainerDir>/sys/fs/cgroup</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:5000]/</WebUI>
  <Icon></Icon>
  <ExtraParams></ExtraParams>
</Containers>
