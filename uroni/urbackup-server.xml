<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Beta>False</Beta>
  <Category>Backup:</Category>
  <Name>uroni-urbackup</Name>
  <Description>
    urbackup - privelaged for btrfs support [br]
  </Description>
  <Overview>
    urbackup with image from uroni/urbackup-server [br]
  </Overview>
  <Support>http://lime-technology.com/forum/index.php</Support>
  <Registry>https://registry.hub.docker.com/u/uroni/urbackup-server</Registry>
  <GitHub>https://github.com/uroni/urbackup-server-docker</GitHub>
  <Repository>uroni/urbackup-server</Repository>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>55413-55415</HostPort>
        <ContainerPort>55413-55415</ContainerPort>
        <Protocol>tcp</Protocol>
	  </Port>	
	  <Port>
		<HostPort>35623</HostPort>
        <ContainerPort>35623</ContainerPort>
        <Protocol>udp</Protocol>
		</Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>DEBUG</Name>
      <Value>false</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/uroni-urbackup</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/backup</HostDir>
      <ContainerDir>/backups</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/uroni-urbackup/database</HostDir>
      <ContainerDir>/var/urbackup</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:55414]/</WebUI>
  <Icon>https://github.com/davans/docker-templates/blob/master/uroni/urbackup.png</Icon>
  <ExtraParams></ExtraParams>
</Containers>
